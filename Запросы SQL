SELECT Contras.Nm_Ct, Telct.TelNum
FROM Contras, Telct
WHERE Telct.Respondent IS NULL;
***********************************
SELECT Contras.Nm_Ct, TelNum.TelNum
FROM Contras,  TelNum
WHERE Contras.CodCt = Telct.CodCt
***********************************
SELECT Contras.Nm_Ct
FROM Contras
JOIN Telct on (Contras.CodCt = Telct.CodCt)
GROUP BY Contras.Nm_Ct
HAVING COUNT (Contras.Nm_Ct)>1
